{
    "name": "ucan-lab/laravel-blink-logger",
    "description": "Comprehensive Logging Tool for Laravel.",
    "keywords": [
        "php",
        "laravel",
        "logger"
    ],
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "LaravelBlinkLogger\\": "src/"
        }
    },
    "authors": [
        {
            "name": "ucan-lab",
            "email": "35098175+ucan-lab@users.noreply.github.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.15|^11.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelBlinkLogger\\Providers\\LaravelBlinkLoggerServiceProvider"
            ]
        }
    },
    "require-dev": {
        "laravel/pint": "^1.15"
    }
}
